    "def FibSeries(Nth):\n",
    "    n1 = 0 \n",
    "    n2 = 1\n",
    "\n",
    "    for i in range(Nth//2):\n",
    "        print(n1 , end = \" \")\n",
    "        print(n2 , end = \" \")\n",
    "        n1 = n1 + n2\n",
    "        n2 = n1 + n2\n",
    "\n",
    "    if Nth%2 == 0 :\n",
    "        return\n",
    "    else:\n",
    "        print(n1 , end = \" \")"
   ]
